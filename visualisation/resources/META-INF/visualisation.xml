<idea-plugin allow-bundled-update="true">

  <extensionPoints>
    <extensionPoint qualifiedName="com.intellij.datavis.r.inlays.inlayDescriptorProvider"
                    interface="org.intellij.datavis.r.inlays.InlayDescriptorProvider"/>
    <extensionPoint qualifiedName="com.intellij.datavis.r.inlays.components.graphicsManagerProvider"
                    interface="org.intellij.datavis.r.inlays.components.GraphicsManagerProvider"/>
  </extensionPoints>

  <project-components>
    <component>
      <implementation-class>org.intellij.datavis.r.inlays.InlaysManager</implementation-class>
    </component>
  </project-components>

</idea-plugin>
