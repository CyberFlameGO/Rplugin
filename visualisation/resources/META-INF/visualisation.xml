<idea-plugin allow-bundled-update="true">

  <extensionPoints>
    <extensionPoint qualifiedName="org.intellij.datavis.inlays.inlayDescriptorProvider"
                    interface="org.intellij.datavis.inlays.InlayDescriptorProvider"/>
    <extensionPoint qualifiedName="org.intellij.datavis.inlays.components.graphicsManager"
                    interface="org.intellij.datavis.inlays.components.GraphicsManager"/>
  </extensionPoints>

  <project-components>
    <component>
      <implementation-class>org.intellij.datavis.inlays.InlaysManager</implementation-class>
    </component>
  </project-components>

</idea-plugin>
